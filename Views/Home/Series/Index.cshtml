@model List<eComMaster.Models.MasterData.PcSeries>

<h1>Pc Series</h1>
<div style="overflow-x: scroll; white-space: nowrap;">
    @foreach (var series in Model)
    {
        <div class="card" style="display: inline-block; margin-right: 10px;">
            <div class="card-header">
                <h5 class="card-title">PC_SERIES_ID: @series.PC_SERIES_ID</h5>
            </div>
            <div class="card-body">
                <h6 class="card-subtitle mb-2 text-muted">PC_SERIES_NAME: @series.PC_SERIES_NAME</h6>
                <p class="card-text">PC_SERIES_DESCRIPTION: @series.PC_SERIES_DESCRIPTION</p>
                <p class="card-text">PC_SERIES_STATUS: @series.PC_SERIES_STATUS</p>
            </div>
        </div>
    }
</div>

